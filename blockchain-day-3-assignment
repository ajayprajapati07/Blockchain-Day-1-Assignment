pragma solidity >=0.4.17 <0.7.0;
    contract Land{
    string Owner;
    
    function Land(string newOwner) public{
      Owner=newOwner;  
    }
    
    function setDetails(string newOwner) public
    {
        Owner=newOwner;
    }
    
    function getDetails() public view returns(string)
    {
        return(Owner);
    }
}
